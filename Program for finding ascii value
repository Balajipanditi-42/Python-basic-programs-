def get_ascii_value(character):
    len(character)==1
    return ord(character)
    
input_character=input("Enter a single charcter of your choice")
ascii_value=get_ascii_value(input_character)
if ascii_value is not None:
    print(f"The ASCII value of '{input_character}' is {ascii_value}")
else:
    print("Please enter a single character.")
